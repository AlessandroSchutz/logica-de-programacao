algoritmo "semnome"
// Função :
// Autor :
// Data : 27/10/2022
// Seção de Declarações 
var
   ano_nasc, ano_atual, idade: inteiro
inicio
// Seção de Comandos 
   escreva("Em que ano nos estamos? ")
   leia(ano_atual)
   escreva("Em que ano eu nasci? ")
   leia(ano_nasc)
   idade <- ano_atual - ano_nasc
   escreva("Eu tenho:", idade, " anos")

fimalgoritmo
